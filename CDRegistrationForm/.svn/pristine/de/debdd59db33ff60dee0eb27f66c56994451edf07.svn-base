body {
  overflow-x:hidden;
}
.loadMask {
  position:fixed;
  top:0;left:0;bottom:0;right:0;
  background:#fff;z-index:9999;
}
.funSelArea {
  width: 10rem;
  padding-top: 10px;
  margin-bottom: 10px;
  background: #fff; 
}
.funSelArea > a {
  width: 2.5rem;
  height: 2.4rem;
  text-align: center;
  font-size: 12px;
}
.funSelArea > a:link {
  color:#666;
}
.funSelArea > a:visited {
  color:#666;
}
.funSelArea > a:active {
  color:#666;
}
.funSelArea > a > span {
  display: block;
  width: 1.28rem;
  height: 1.28rem;
  margin: 0 auto 3px;
  background-size: 100% 100%;
}
.funSelArea > li {
  width: 2.5rem;
  height: 2.4rem;
  text-align: center;
  font-size: 12px; }

.funSelArea > li > a {
  display: block;
  width: 1.28rem;
  height: 1.28rem;
  margin: 0 auto 3px;
  background-size: 100% 100%; }

.funSelArea .lh01 {
  line-height:0.4266666666666667rem; }

.funSelArea .lh02 {
  font-size: 10px;
  line-height:0.42rem;
  white-space: nowrap;
  overflow:hidden;
  text-overflow: ellipsis;
}

.conBox {
  margin-bottom:10px;
  background: #fff;
  font-size:12px;
}

.conBox .title {
  height:44px;
  line-height:44px;
  padding-left:15px;
  border-bottom: 1px solid #b4b8c4;
  color: #333;
  font-size:14px; 
}

.conBox li > a {
  color: #666; 
}

.conBox .title > .wordTrans {
  font-size: 12px;
  margin-left: 10px; 
}
.listInfoOne,.listInfoTwo {
  padding:0 15px;
}
.listInfoOne > li,.listInfoTwo > a {
  height:50px;
  line-height:50px;
  border-bottom: 1px solid #ededed; }

.listInfoOne > li > i {
  width:24px;
  height:22px;
  margin-top:14px;
  background: url("../img/downLoadIcon.png") no-repeat;
  background-size:100% 100%; 
}

.listInfoTwo > a {
  display:block;height:44px;line-height:44px;color:#666;
}
.listInfoTwo p.noticeTxt,.listInfoOne > li > span {
  width:70%;height:100%;
  white-space: nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
}
.listInfoTwo span.dateTime {
  color:#999;font-size:10px;
}
.listInfoOne > li:last-child, .listInfoTwo > a:last-child {
  border-bottom: none; }

.swiper-container {
  width:10rem;
  height:4.773333333333333rem;
  margin:0;
}
.swiper-slide {
  width:80%;
}
.swiper-slide a {
  display:block;width:100%;height:100%;
}
.swiper-slide a > img {
  width:100%;height:100%;
}
.swiper-pagination.swiper-pagination-bullets {
  bottom:0;
}
.swiper-pagination-bullet {
   width:8px;height:8px;opacity:1;border:1px solid #ddedf8;
   background:none;
}
.swiper-pagination-bullet-active {
  background:#ddedf8;
}
.swiper-container-3d {
    -webkit-perspective: 800px;
    -moz-perspective: 800px;
    -o-perspective: 800px;
    perspective: 800px;
}
/*# sourceMappingURL=homePage.css.map */
